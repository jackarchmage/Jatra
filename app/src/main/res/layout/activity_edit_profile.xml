<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditProfile">

    <FrameLayout
        android:id="@+id/container_iphone_pro_max"
        android:layout_width="match_parent"
        android:layout_height="844dp"
        android:background="@color/white">

        <LinearLayout
            android:id="@+id/container_frame"
            android:layout_width="356dp"
            android:layout_height="30dp"
            android:layout_marginLeft="-448dp"
            android:layout_marginRight="482dp"
            android:layout_marginTop="317dp"
            android:layout_marginBottom="497dp"
            android:clipToPadding="false"
            android:gravity="left|center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="7dp"
            android:paddingTop="7dp">

            <TextView
                android:id="@+id/text_lastest_works"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_marginTop="-7dp"
                android:layout_marginBottom="-7dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Lastest works"
                android:fontFamily="@font/interweight"
                android:textColor="#FF000000"
                android:textSize="24dp" />
        </LinearLayout>

        <ImageView
            android:id="@+id/image_ellipse"
            android:layout_width="83dp"
            android:layout_height="83dp"
            android:layout_marginRight="0.5dp"
            android:layout_marginTop="131dp"
            android:layout_gravity="top|center_horizontal"
            android:background="@drawable/profileroundbg"
            android:scaleType="centerInside" />

        <FrameLayout
            android:id="@+id/container_frame1"
            android:layout_width="342dp"
            android:layout_height="53dp"
            android:layout_marginLeft="23dp"
            android:layout_marginTop="237dp"
            android:layout_gravity="top|left">

            <TextView
                android:id="@+id/text_emily_sezehn"
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:layout_marginLeft="3.33dp"
                android:layout_gravity="center_horizontal"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Emily Sezehn"
                android:textColor="#FF282828"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/text_email"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginLeft="2.9dp"
                android:layout_marginTop="29.07dp"
                android:layout_gravity="center_horizontal"
                android:gravity="center_horizontal|top"
                android:includeFontPadding="false"
                android:lineHeight="14.52dp"
                android:fontFamily="@font/interweight"
                android:text="xxxx@example.com"
                android:textColor="#FF282828"
                android:textSize="12dp" />
        </FrameLayout>

        <LinearLayout
            android:id="@+id/container_frame2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="26.89dp"
            android:layout_marginTop="315dp"
            android:layout_gravity="top|left"
            android:clipToPadding="false"
            android:gravity="left|top"
            android:orientation="vertical"
            android:paddingTop="1dp">

            <TextView
                android:id="@+id/text_full_name"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_marginTop="-1dp"
                android:layout_marginBottom="-1dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Full Name"
                android:fontFamily="@font/interweight"
                android:textColor="#FF282828"
                android:textSize="14.31dp" />

            <EditText
                android:id="@+id/text_emily_sezehn1"
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="7.16dp"
                android:background="@drawable/textviewbg"
                android:gravity="left|center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="11.36dp"
                android:lines="1"
                android:textColor="@color/black"
                android:paddingTop="11.36dp">

            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_frame4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="26.89dp"
            android:layout_marginTop="405.35dp"
            android:layout_gravity="top|left"
            android:clipToPadding="false"
            android:gravity="left|top"
            android:orientation="vertical"
            android:paddingTop="1dp">

            <TextView
                android:id="@+id/text_email_label"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_marginTop="-1dp"
                android:layout_marginBottom="-1dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Email"
                android:fontFamily="@font/interweight"
                android:textColor="#FF282828"
                android:textSize="14.31dp" />

            <EditText
                android:id="@+id/text_user_email"
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="7.16dp"
                android:background="@drawable/textviewbg"
                android:clipToPadding="false"
                android:gravity="left|center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="11.36dp"
                android:textColor="@color/black"
                android:paddingTop="11.36dp">

            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_frame6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="26dp"
            android:layout_marginTop="495.7dp"
            android:layout_gravity="top|left"
            android:clipToPadding="false"
            android:gravity="left|top"
            android:orientation="vertical"
            android:paddingTop="1dp">

            <TextView
                android:id="@+id/text_date_of_birth"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_marginTop="-1dp"
                android:layout_marginBottom="-1dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:fontFamily="@font/interweight"
                android:singleLine="true"
                android:text="Date of birth"
                android:textColor="#FF282828"
                android:textSize="14.31dp" />

            <LinearLayout
                android:id="@+id/container_frame7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7.16dp"
                android:background="@drawable/textviewbg"
                android:clipToPadding="false"
                android:gravity="left|top"
                android:orientation="horizontal"
                android:paddingBottom="8.95dp"
                android:paddingLeft="16.1dp"
                android:paddingRight="16.1dp"
                android:paddingTop="8.95dp">

                <TextView
                    android:id="@+id/text_birth_date"
                    android:layout_width="wrap_content"
                    android:layout_height="22dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="12/02/1998"
                    android:fontFamily="@font/interweight"
                    android:textColor="#FF282828"
                    android:textSize="16.1dp" />

                <ImageView
                    android:id="@+id/image_date_range"
                    android:layout_width="21.47dp"
                    android:layout_height="21.47dp"
                    android:layout_marginLeft="16.1dp"
                    android:scaleType="centerInside"
                    android:src="@drawable/outline_date_range_24" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_frame8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="204.91dp"
            android:layout_marginTop="496.59dp"
            android:layout_gravity="top|left"
            android:clipToPadding="false"
            android:gravity="left|top"
            android:orientation="vertical"
            android:paddingTop="1dp">

            <TextView
                android:id="@+id/text_gender"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_marginTop="-1dp"
                android:layout_marginBottom="-1dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:fontFamily="@font/interweight"
                android:singleLine="true"
                android:text="Gender"
                android:textColor="#FF282828"
                android:textSize="14.31dp" />

            <LinearLayout
                android:id="@+id/container_frame9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7.16dp"
                android:background="@drawable/textviewbg"
                android:clipToPadding="false"
                android:gravity="left|top"
                android:orientation="horizontal"
                android:paddingBottom="8.95dp"
                android:paddingLeft="16.1dp"
                android:paddingRight="93.03dp"
                android:paddingTop="8.95dp">

                <TextView
                    android:id="@+id/text_female"
                    android:layout_width="wrap_content"
                    android:layout_height="22dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Female"
                    android:fontFamily="@font/interweight"
                    android:textColor="#FF282828"
                    android:textSize="16.1sp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_frame10"
            android:layout_width="390dp"
            android:layout_height="104dp"
            android:layout_gravity="top|center_horizontal"
            android:gravity="left|top"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/container_frame12"
                android:layout_width="360dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="-3dp"
                android:layout_gravity="center_horizontal"
                android:clipToPadding="false"
                android:gravity="left|center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="3dp"
                android:paddingTop="3dp">

                <ImageView
                    android:id="@+id/image_arrow_left"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:scaleType="centerInside"
                    android:src="@drawable/baseline_arrow_back_24" />

                <TextView
                    android:id="@+id/text_my_profile"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="14dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:fontFamily="@font/interweight"
                    android:text="My Profile"
                    android:textColor="#FF151515"
                    android:textSize="24sp" />
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>